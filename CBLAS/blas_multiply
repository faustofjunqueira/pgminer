-- Function: pgm_matrix_add(bigint, bigint)

-- DROP FUNCTION blas_multiply(bigint, bigint);

CREATE OR REPLACE FUNCTION blas_multiply(A float8[], B float8[])
  RETURNS float8[] AS
'$libdir/pgminer.so', 'blas_multiply'
  LANGUAGE c VOLATILE STRICT
  COST 1;
ALTER FUNCTION pgm_matrix_add(bigint, bigint)
  OWNER TO gccbd;

select blas_multiply(array[[ 0.11,0.12,0.13],
			   [ 0.21,0.22,0.23]],
			      
		     array[[1011,1012],
			   [1021,1022],
			   [1031,1032]]) 